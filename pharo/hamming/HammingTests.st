'From Pharo6.0 of 13 May 2016 [Latest update: #60501] on 10 March 2018 at 4:05:27.273657 pm'!TestCase subclass: #HammingTests	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'Exercism-Tests'!!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/9/2018 07:29'!testIdenticalStrands	| distance |	distance := HammingDistance between: 'A' and: 'A'.	self assert: distance value equals: 0! !!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/9/2018 07:28'!testCompleteDistanceInSingleNucleotideStrands	| distance |	distance := HammingDistance between: 'A' and: 'G'.	self assert: distance value equals: 1! !!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/9/2018 07:29'!testLargeDistance	| distance |	distance := HammingDistance between: 'GATACA' and: 'GCATAA'.	self assert: distance value equals: 4! !!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/9/2018 07:28'!testLargeDistanceInOffByOneStrand	| distance |	distance := HammingDistance		between: 'GGACGGATTCTG'		and: 'AGGACGGATTCT'.	self assert: distance value equals: 9! !!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/9/2018 07:30'!testNonUniqueCharacterInFirstStrand	| distance |	distance := HammingDistance between: 'AAA' and: 'AAG'.	self assert: distance value equals: 1! !!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/10/2018 15:43'!testDisallowerLongerFirstStrand	self		should: [  HammingDistance between: 'AATG' and: 'AAA' ]		raise: AssertionFailure! !!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/8/2018 23:23'!testSmallDistanceInSmallStrands	| distance |	distance := HammingDistance between: 'AT' and: 'CT'.	self assert: distance value = 1! !!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/10/2018 15:43'!testDisallowerLongerSecondStrand	self		should: [  HammingDistance between: 'ATA' and: 'AGTG' ]		raise: AssertionFailure! !!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/9/2018 07:29'!testCompleteDistanceInSmallStrands	| distance |	distance := HammingDistance between: 'AG' and: 'CT'.	self assert: distance value equals: 2! !!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/8/2018 23:25'!testSameNucleotidesInDifferentPositions	| distance |	distance := HammingDistance between: 'TAG' and: 'GAT'.	self assert: distance value = 2! !!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/8/2018 23:24'!testSmallDistance	| distance |	distance := HammingDistance between: 'GGACG' and: 'GGTCG'.	self assert: distance value = 1! !!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/9/2018 07:29'!testLongIdenticalStrands	| distance |	distance := HammingDistance between: 'GGACTGA' and: 'GGACTGA'.	self assert: distance value equals: 0! !!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/8/2018 23:24'!testSmallDistanceInLongStrands	| distance |	distance := HammingDistance between: 'ACCAGGG' and: 'ACTATGG'.	self assert: distance value = 2! !!HammingTests methodsFor: 'tests' stamp: 'TomSmall 3/9/2018 07:29'!testEmptyStrands	| distance |	distance := HammingDistance between: '' and: ''.	self assert: distance value equals: 0! !